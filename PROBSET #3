#include <iostream>
#include <string>
using namespace std;

int main() {
    string response;

    cout << "Do you want to print a countdown? (yes/no): ";
    cin >> response;

    if (response == "yes") {
        for (int i = 10; i >= 1; i--) {
            cout << i << endl;
        }
    } else if (response == "no") {
        cout << "Maybe next time!" << endl;
    } else {
        cout << "Invalid input." << endl;
    }

    return 0;
}
